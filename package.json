{
  "name": "react-vite-ts-template",
  "version": "0.0.0",
  "description": "A Vite with React and TypeScript starter template. Included eslint, prettier, husky and lint-staged.",
  "homepage": "https://github.com/teziovsky/react-vite-ts-template/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/react-vite-ts-template.git"
  },
  "engines": {
    "node": ">=v16.0.0",
    "npm": ">=v8.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/react-vite-ts-template/issues",
    "email": "teziovsky@gmail.com"
  },
  "license": "MIT License",
  "author": {
    "name": "Jakub Soboczy≈Ñski",
    "email": "teziovsky@gmail.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --quiet",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx,scss,json,md}": "prettier --cache --write",
    "*.{jsx,js,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
